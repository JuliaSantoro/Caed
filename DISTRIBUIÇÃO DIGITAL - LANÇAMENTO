SELECT DISTINCT NM_ETAPA, COUNT(DISTINCT CD_SUJEITO) AS 'ALUNO', COUNT(DISTINCT CD_REGISTRO_CAED) AS 'SEQUENCIAL'
	FROM ARQ_AD_055_D_2037_20250311163536_000062754  
	GROUP BY NM_ETAPA
	ORDER BY 1



SELECT COUNT(DISTINCT CD_SUJEITO) AS 'ALUNO', COUNT(DISTINCT CD_REGISTRO_CAED) AS 'SEQUENCIAL'
	FROM ARQ_AD_055_D_2037_20250311163536_000062754  


SELECT * 
	FROM ##UNIDADES A
	WHERE NU_SEQUENCIAL NOT IN (SELECT DISTINCT CD_REGISTRO_CAED
									FROM ARQ_AD_055_D_2037_20250311163536_000062754 B
									WHERE A.NU_SEQUENCIAL = B.CD_REGISTRO_CAED)


SELECT * 
	FROM ARQ_DA_005_D_1407_20250404142855_001637179 A
	WHERE NU_SEQUENCIAL NOT IN (SELECT DISTINCT CD_REGISTRO_CAED
									FROM ARQ_AD_055_D_2037_20250311163536_000062754 B
									WHERE A.NU_SEQUENCIAL = B.CD_REGISTRO_CAED)


SELECT DISTINCT NM_ETAPA,COUNT (DISTINCT CD_SALA_DE_APLICACAO),
		 COUNT (DISTINCT CD_SUJEITO) ALUNOS,
		 COUNT (DISTINCT CD_REGISTRO_CAED)	SEQUENCIAIS
	FROM ARQ_AD_055_D_2037_20250311163536_000062754
	WHERE TP_LANCAMENTO = '1'
	GROUP BY NM_ETAPA
	ORDER BY 1



