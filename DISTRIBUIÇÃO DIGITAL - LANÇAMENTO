------------------------------------------------------------------------------------------------------------- TOTAIS --------------------------------------------------------------------------------------------------------------------------------------
SELECT CD_AVALIACAO_ALUNO, COUNT (DISTINCT CD_ALUNO), COUNT (DISTINCT NU_SEQUENCIAL)
	FROM ##UNIDADES	A
	GROUP BY CD_AVALIACAO_ALUNO
-- 458988	917976


SELECT DISTINCT CD_AVALIACAO, COUNT (DISTINCT CD_SUJEITO_CAED), COUNT (DISTINCT NU_SEQUENCIAL)
	FROM ARQ_DA_005_D_2034_20250221133224_001076630	
	GROUP BY CD_AVALIACAO



SELECT DISTINCT DC_ETAPA_TURMA,NM_DISCIPLINA,
		 COUNT (DISTINCT CD_ALUNO),
		 COUNT (DISTINCT NU_SEQUENCIAL)	
	FROM ##UNIDADES
	GROUP BY DC_ETAPA_TURMA,NM_DISCIPLINA
	ORDER BY 1, 2, 3


SELECT DISTINCT DC_ETAPA_TURMA,
		 COUNT (DISTINCT CD_ALUNO) ALUNOS,
		 COUNT (DISTINCT NU_SEQUENCIAL)	SEQUENCIAIS
	FROM ##UNIDADES
	GROUP BY DC_ETAPA_TURMA
	ORDER BY 1, 2

SELECT DISTINCT DC_ETAPA_SECRETARIA,
		 COUNT (DISTINCT CD_SUJEITO_CAED) ALUNOS,
		 COUNT (DISTINCT NU_SEQUENCIAL)	SEQUENCIAIS
	FROM ARQ_DA_005_D_1407_20250404142855_001637179
	GROUP BY DC_ETAPA_SECRETARIA
	ORDER BY 1, 2


SELECT *
	FROM ##UNIDADES A
	WHERE NU_SEQUENCIAL NOT IN (SELECT DISTINCT NU_SEQUENCIAL 
									FROM ARQ_DA_005_D_1407_20250404142855_001637179 B 
									WHERE A.NU_SEQUENCIAL = B.NU_SEQUENCIAL)


SELECT *
	FROM ARQ_DA_005_D_1407_20250404142855_001637179 A
	WHERE NU_SEQUENCIAL NOT IN (SELECT DISTINCT NU_SEQUENCIAL 
									FROM ##UNIDADES B 
									WHERE A.NU_SEQUENCIAL = B.NU_SEQUENCIAL)



SELECT DISTINCT DC_ETAPA_TURMA,COUNT (DISTINCT CD_TURMA),
		 COUNT (DISTINCT CD_ALUNO) ALUNOS,
		 COUNT (DISTINCT NU_SEQUENCIAL)	SEQUENCIAIS
	FROM ##UNIDADES
	WHERE TP_EXTRA = '1'
	GROUP BY DC_ETAPA_TURMA
	ORDER BY 1
	
	------------------------------------------------------------------------------------------------------------- DISTRIBUIDO ---------------------------------------------------------------------------------------------------------------------------------------

/*
SELECT TOP 0 * INTO  ARQ_AD_055_D_2037_20250311163536_000062754 
	FROM SDV_SAETO_1_FORMATIVA_2025.dbo.ARQ_AD_055_D_1480_20250225180436_000264472

BULK INSERT ARQ_AD_055_D_2037_20250311163536_000062754   
	FROM '\\192.168.250.8\bulk\ARQ_AD_055_D_2037_20250311163536_000062754.csv'
